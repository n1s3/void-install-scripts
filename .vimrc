set number
syntax on
set nocompatible
filetype plugin on
filetype indent on
set shiftwidth=4
set tabstop=4
set expandtab
set nobackup
set nowrap
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsxi

call plug#begin()

Plug 'lilydjwg/colorizer'

call plug#end()
